<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhxh.thirdParty.wyl.mapper.DeviceStateMapper">
    <resultMap type="com.zhxh.thirdParty.wyl.domain.DeviceState" id="DeviceStateResultMap">
        <result property="id" column="ID"/>
        <result property="gid" column="GID"/>
        <result property="did" column="DID"/>
        <result property="state" column="State"/>
        <result property="updateTime" column="UpdateTime"/>
    </resultMap>

    <select id="getAll" resultMap="DeviceStateResultMap">
        select ID,GID,DID,State,UpdateTime from DeviceState
    </select>
</mapper>